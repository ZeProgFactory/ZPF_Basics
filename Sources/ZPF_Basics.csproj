<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Michael Engelmann</Authors>
    <Company>ZPF (ME, CHM)</Company>
    <Copyright>Copyright 2017..2020</Copyright>
    <PackageLicenseUrl></PackageLicenseUrl>
    <PackageProjectUrl>http://www.ZPF.fr</PackageProjectUrl>
    <PackageIconUrl>https://raw.githubusercontent.com/ZeProgFactory/MediaPlayer/master/docs/ZeProgramFactory.Square.BG.png</PackageIconUrl>
    <PackageTags>Basics</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.2.0.1</Version>
    <Description>Basics ( V2 / .Net Standard 2.0 )</Description>
    <AssemblyVersion>1.2.0.1</AssemblyVersion>
    <FileVersion>1.2.0.1</FileVersion>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/ZeProgFactory/ZPF_Basics</RepositoryUrl>
    <RepositoryType>GIT</RepositoryType>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <DefineConstants>TRACE;DEBUG;NETSTANDARD2_0;DBI</DefineConstants>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugType></DebugType>
    <DefineConstants>TRACE;RELEASE;NETSTANDARD2_0;DBI</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="CS\DoIt_UT.cs" />
    <Compile Remove="CS\TIniFile_UT.cs" />
    <Compile Remove="CS\TStrings_UT.cs" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="..\..\..\_Units_\AuditTrail\AuditTrail.cs" Link="AuditTrail\AuditTrail.cs" />
    <Compile Include="..\..\..\_Units_\AuditTrail\AuditTrailViewModel.cs" Link="AuditTrail\AuditTrailViewModel.cs" />
    <Compile Include="..\..\..\_Units_\AuditTrail\AuditTrail_Appli.cs" Link="AuditTrail\AuditTrail_Appli.cs" />
    <Compile Include="..\..\..\_Units_\AuditTrail\DebugAuditTrailWriter.cs" Link="AuditTrail\DebugAuditTrailWriter.cs" />
    <Compile Include="..\..\..\_Units_\AuditTrail\IAuditTrailWriter.cs" Link="AuditTrail\IAuditTrailWriter.cs" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="DBSQL\" />
    <Folder Include="AuditTrail\" />
    <Folder Include="ViewModels\" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="System.Data.Common" Version="4.3.0" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Update="NETStandard.Library" Version="2.0.1" />
  </ItemGroup>
</Project>
